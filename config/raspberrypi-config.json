    {
      "api_path": "/api/commands/ntpq",
      "html_path": "/commands/ntpq",
      "description": "ntpq",
      "command": "ntpq",
      "args": [
        "-p"
      ]
    },
    {
      "api_path": "/api/commands/pitemp",
      "html_path": "/commands/pitemp",
      "description": "pitemp",
      "command": "/home/pi/bin/pitemp.sh",
      "args": []
