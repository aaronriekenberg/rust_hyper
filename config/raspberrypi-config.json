      "html_path": "/commands/tc_qdisc",
      "description": "tc qdisc show",
      "command": "/sbin/tc",
      "args": [
        "-s",
        "qdisc",
        "show"
      ]
    },
    {
      "api_path": "/api/commands/top",
      "html_path": "/commands/top",
